{
  "name": "godash",
  "version": "0.4.2",
  "description": "Immutable data structures and utilities to represent Go",
  "homepage": "https://github.com/duckpunch/godash",
  "repository": {
    "type": "git",
    "url": "https://github.com/duckpunch/godash.git"
  },
  "main": "index.js",
  "keywords": [
    "go",
    "weiqi",
    "baduk",
    "immutable"
  ],
  "author": {
    "name": "duckpunch",
    "url": "https://github.com/duckpunch"
  },
  "license": "ISC",
  "dependencies": {
    "immutable": "^3.7.5",
    "immutable-schema": "^0.3.0",
    "lodash": "^4.5.1"
  },
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^11.0.1",
    "mochify": "^2.13.0",
    "uglify-js": "^2.4.24"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "scripts": {
    "build": "browserify -o ./build/godash.js -e ./browser.js && uglifyjs --compress --mangle --output build/godash.min.js -- build/godash.js",
    "build-dist": "browserify -o ./dist/godash.js -e ./browser.js && uglifyjs --compress --mangle --output dist/godash.min.js -- dist/godash.js",
    "test": "mochify",
    "watch-test": "mochify -w"
  }
}
